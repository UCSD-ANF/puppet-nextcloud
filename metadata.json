{
  "name": "puppet-nextcloud",
  "version": "0.1.2",
  "author": "Adam Brust",
  "summary": "Puppet module for installing Nextcloud, supports nginx,mysql,php",
  "license": "Apache-2.0",
  "source": "https://github.com/UCSD-ANF/puppet-nextcloud",
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 4.25.0 < 9.0.0"
    },
    {
      "name": "stahnma/epel",
      "version_requirement": ">= 1.2.2 < 2.0.0"
    },
    {
      "name": "puppetlabs-mysql",
      "version_requirement": ">= 5.0.0"
    },
    {
      "name": "puppet-php",
      "version_requirement": ">= 6.0.0 <= 7.0.0"
    },
    {
      "name": "puppet-nginx",
      "version_requirement": ">= 0.12.0"
    },
    {
      "name": "puppet-archive",
      "version_requirement": ">= 3.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "7"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "7"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 4.10.0 < 8.0.0"
    }
  ],
  "pdk-version": "2.7.1",
  "template-url": "https://github.com/puppetlabs/pdk-templates#2.7.4",
  "template-ref": "tags/2.7.4-0-g58edf57"
}
